{
    "name":  "custom-monorepo",
    "version":  "0.0.0",
    "private":  true,
    "type":  "module",
    "packageManager":  "pnpm@10.20.0",
    "scripts":  {
                    "build:docs":  "pnpm --filter @workspace/docs build",
                    "dev:slides":  "pnpm --filter @workspace/slides dev",
                    "export:slides:png":  "pnpm --filter @workspace/slides export:png",
                    "build":  "turbo run build",
                    "dev:web":  "pnpm --filter @custom/web dev",
                    "build:slides":  "pnpm --filter @workspace/slides build",
                    "test":  "turbo run test",
                    "dev:docs":  "pnpm --filter @workspace/docs dev",
                    "preview:docs":  "pnpm --filter @workspace/docs preview",
                    "export:slides:pdf":  "pnpm --filter @workspace/slides export:pdf",
                    "dev":  "turbo run dev",
                    "lint":  "pnpm run lint:ts \u0026\u0026 pnpm run lint:md \u0026\u0026 pnpm run lint:prettier",
                    "lint:ts":  "eslint -c eslint.config.mjs --max-warnings=0 \"docs/.vitepress/**/*.{ts,js,mts,cts}\" \"docs/.vitepress/theme/**/*.{ts,js}\"",
                    "lint:md":  "markdownlint-cli2 \"docs/**/*.md\" \"slides/**/*.md\" \"!docs/node_modules/**\"",
                    "lint:prettier":  "prettier --check \"docs/**/*.*\" \"slides/**/*.*\" \"docs/.vitepress/**/*.*\"",
                    "format":  "prettier -w \"docs/**/*.*\" \"slides/**/*.*\" \"docs/.vitepress/**/*.*\"",
                    "fix:md":  "node scripts/fix-md-lowrisk.mjs"
                },
    "devDependencies":  {
                            "@eslint/js":  "^9.12.0",
                            "@icebreakers/eslint-config":  "^1.5.9",
                            "eslint":  "^9.12.0",
                            "iconv-lite":  "^0.6.3",
                            "markdownlint-cli2":  "^0.17.2",
                            "prettier":  "^3.3.3",
                            "turbo":  "^2.6.0",
                            "typescript-eslint":  "^8.11.0"
                        },
    "pnpm":  {

             }
}